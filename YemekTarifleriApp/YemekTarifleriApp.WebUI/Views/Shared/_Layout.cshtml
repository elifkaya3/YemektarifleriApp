<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - YemekTarifleriApp.WebUI</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/css/style.css" rel="stylesheet" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-warning border-bottom box-shadow mb-3 " style="background-color: #f0932b;">
            <div class="container">

                

                <a class="navbar-brand text-white" asp-area="" asp-controller="Home" asp-action="Index">Elifin Yemek Tarifleri</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">Anasayfa</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Recipe" asp-action="List">Tarifler</a>
                        </li>
                        @*<li class="nav-item">
                         <a class="nav-link text-white" asp-area="" asp-controller="Member" asp-action="Index">Yazarlar</a>
                         </li>*@
                        <li>     </li>
                        <li>     </li>
                        <li>     </li>
                        <li class="nav-item">
                            <a href="/admin/recipelist" class="nav-link text-white">Admin's Recipe Actions</a>
                        </li>
                        <li class="nav-item">
                            <a href="/admin/categorylist" class="nav-link text-white">Admin's Category Actions</a>
                        </li>
                        <li class="nav-item">
                            <a href="/admin/memberlist" class="nav-link text-white">Admin's Member Actions</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer >
        <div class="  liste">
            <div>
                <ul>
                    <li>ANASAYFA</li>
                    <li>HAKKINDA</li>
                    <li>İLETİŞİM</li>
                </ul>
            </div>
            <div>
                <ul>
                    <li>TARİFLER</li>
                    <li>KURABİYE TARİFLERİ</li>
                    <li>SEBZE TARİFLERİ</li>
                </ul>
            </div>
            <div>
                <ul>
                    <li>YAZARLAR</li>
                    <li>TATLI TARİFLERİ</li>
                    <li>ELMALI KURABİYE</li>
                </ul>
            </div>
            <div class="ikon">
                <a href="#" class="fab fa-instagram"></a>
                <a href="#" class="fab fa-facebook-f"></a>
                <a href="#" class="fab fa-youtube"></a>
                <a href="#" class="fab fa-twitter"></a>
            </div>
            <div>
                &copy; 2022 - YemekTarifleriApp.WebUI - <a asp-area="" asp-controller="Home" asp-action="Index">Anasayfaya Dön</a>

            </div>
        </div>

    </footer>


    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>*@
    @*<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>*@
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/js/all.min.js" integrity="sha512-6PM0qYu5KExuNcKt5bURAoT6KCThUmHRewN3zUFNaoI6Di7XJPTMoT6K0nsagZKk2OB4L7E3q1uQKHNHd4stIQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
